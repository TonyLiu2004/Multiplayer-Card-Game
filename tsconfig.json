{
    "compilerOptions": {
      "sourceMap": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "allowSyntheticDefaultImports": true,
      "allowUnreachableCode": false,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "module": "commonjs",
      "target": "es5",
      "allowJs": true,
      "moduleResolution": "node",
      "lib": ["dom", "es5", "esnext"],
      "skipLibCheck": true
    },
    "exclude": ["node_modules", "pwa", "webpack"]
  }